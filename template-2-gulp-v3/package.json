{
  "name": "DF_Project",
  "author": "DesignFever VID",
  "copyright": "DF",
  "version": "1.1.0",
  "description": "DF Build System",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "gulp-dev": "gulp",
    "gulp-dev-js": "gulp js:dev",
    "gulp-build": "gulp build",
    "gulp-del": "gulp clean"
  },
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "iOS >= 9"
  ],
  "paths": {
    "work_src": "./src/",
    "src": {
      "css": "assets/css/",
      "fonts": "assets/fonts/",
      "js": "assets/js/",
      "img": "assets/images/",
      "scss": "assets/scss/",
      "html": "views/",
      "favicon": "favicon/",
      "json": "temp_data/json/"
    },
    "dist_mode": {
      "dev": "./dist/dev/",
      "product": "./dist/prd/"
    },
    "build": {
      "base": "./build_temporary/",
      "js": "./build_temporary/assets/js/",
      "img": "./build_temporary/assets/img/"
    },
    "start_serve_html": "index.html"
  },
  "filelist": {
    "vendorCss": ["./src/assets/vendors/css/**"],
    "workScss": [
      "./src/assets/scss/global.scss",
      "./src/assets/scss/app.scss"
    ],
    "vendorJs": [
      "./src/assets/vendors/js/jquery-3.4.1.min.js",
      "./src/assets/vendors/js/modernizr-custom.js",
      "./src/assets/vendors/js/detectizr.js"
    ],
    "commonJs": [
      "./src/assets/js/debuger/css.debuger.js",
      "./src/assets/js/interactive/frame/GlobalVars.js",
      "./src/assets/js/interactive/app.js",
      "./src/assets/js/interactive/frame/util.main.js"
    ],
    "moduleJs": [
      "./src/assets/js/interactive/module_name_1/**/*.js",
      "./src/assets/js/interactive/module_name_2/**/*.js"
    ],
    "buildCommonJs": [
      "./build_temporary/assets/js/GlobalVars.js",
      "./build_temporary/assets/js/common.js"
    ],
    "onlyCopyJs": [],
    "onlyCopyEmail": [],
    "debugerJs": [],
    "fonts": [
      "./src/assets/fonts/*.{eot,ttf,woff,woff2}"
    ],
    "html": [
      "./src/views/**/**/*.html"
    ]
  },
  "urls": {
    "live": "http://live.sitename.com/",
    "dev": "http://dev.sitename.com/",
    "critical": "http://sitename.com/"
  },
  "varsFileNames": {
    "aliasKeyword": "df",
    "siteVendorsCssName": "vendors.css",
    "siteVendorsJsName": "vendors.js",
    "siteCommonJsName": "common.js",
    "siteDebugerJsName": "debuger.js",
    "siteModuleJsName": [
      "module_name_1",
      "module_name_2"
    ],
    "templateHtmlName": ""
  },
  "gulp": {
    "port": 2020
  },
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.12.10",
    "chalk": "^2.4.1",
    "critical": "^1.3.4",
    "del": "^4.1.1",
    "fancy-log": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-css-replace-url": "^0.2.4",
    "gulp-cssmin": "^0.2.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^4.0.0",
    "gulp-favicons": "^2.4.0",
    "gulp-filter": "^5.0.1",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.1",
    "gulp-ignore": "^2.0.2",
    "gulp-image": "^4.3.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-inject-string": "^1.1.2",
    "gulp-livereload": "^4.0.1",
    "gulp-load-plugins": "^1.6.0",
    "gulp-newer": "^1.2.0",
    "gulp-nunjucks-render": "^2.2.3",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-streamify": "1.0.2",
    "gulp-strip-debug": "^3.0.0",
    "gulp-template-html": "^0.2.2",
    "gulp-uglify": "^3.0.2",
    "moment": "^2.14.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
